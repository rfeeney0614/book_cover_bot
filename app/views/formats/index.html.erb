<h1>Formats</h1>
<div class="relative overflow-x-auto shadow-md sm:rounded-lg">
  <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
  <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
    <tr>
      <th>Name</th>
      <th>Height </th>
    </tr>
  </thead>
  <tbody>
    <%= render @formats %>
  </tbody>
 </table>
 </div>
 <div class="h-40 grid grid-cols-3 gap-1 content-evenly">
 <%= link_to "Add New", new_format_path, class:"text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800" %>
</div>
